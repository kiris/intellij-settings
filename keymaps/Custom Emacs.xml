<keymap version="1" name="Custom Emacs" parent="Emacs">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt minus" />
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
  </action>
  <action id="Back">
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl meta b" />
  </action>
  <action id="CommentByBlockComment" />
  <action id="ContextHelp" />
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
  </action>
  <action id="EditorBackSpace">
    <keyboard-shortcut first-keystroke="back_space" />
    <keyboard-shortcut first-keystroke="shift back_space" />
    <keyboard-shortcut first-keystroke="ctrl h" />
  </action>
  <action id="EditorPasteSimple">
    <keyboard-shortcut first-keystroke="shift ctrl y" />
  </action>
  <action id="Forward">
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="ctrl meta f" />
  </action>
  <action id="InsertLiveTemplate">
    <keyboard-shortcut first-keystroke="enter" />
  </action>
  <action id="RecentFiles">
    <keyboard-shortcut first-keystroke="ctrl alt r" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="meta alt l" />
  </action>
  <action id="ShowPopupMenu">
    <keyboard-shortcut first-keystroke="f1" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="ctrl alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="ctrl alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="ctrl alt up" />
  </action>
  <action id="TypeHierarchy" />
</keymap>